<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>build-with-dependencies</actionName>
            <basedir>.</basedir>
            <goals>
                <goal>reactor:make</goal>
            </goals>
            <properties>
                <make.artifacts>${groupId}:${artifactId}</make.artifacts>
            </properties>
        </action>
        <action>
            <actionName>build</actionName>
            <goals>
                <goal>compile</goal>
                <goal>jar:jar</goal>
            </goals>
        </action>
        <action>
            <actionName>rebuild</actionName>
            <goals>
                <goal>clean</goal>
                <goal>compile</goal>
                <goal>jar:jar</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-Install</actionName>
            <displayName>Install</displayName>
            <goals>
                <goal>install</goal>
            </goals>
        </action>
    </actions>
