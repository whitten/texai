
<assembly xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.0 http://maven.apache.org/xsd/assembly-1.1.0.xsd">
  <id></id>
  <formats>
    <format>zip</format>
  </formats>
  <dependencySets>
    <dependencySet>
      <unpack>false</unpack>
      <scope>runtime</scope>
      <outputDirectory>lib</outputDirectory>
    </dependencySet>
  </dependencySets>
  <files>
    <file>
      <source>src/main/resources/log4j.properties</source>
    </file>
    <file>
      <source>scripts/run-bootstrap-setup.sh</source>
    </file>
    <file>
      <source>scripts/run-texai-launcher.sh</source>
    </file>
    <file>
      <source>../Main/scripts/run-texai-main.sh</source>
    </file>
    <file>
      <source>../Main/scripts/run-texai-main-debug.sh</source>
    </file>
  </files>
  <fileSets>
    <fileSet>
      <directory>../Main/data</directory>
      <includes>
        <include>bootstrap-queries.xml</include>
        <include>node-types.xml</include>
        <include>repository.rcd</include>
        <include>role-types.xml</include>
      </includes>
      <outputDirectory>data</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>doc/graph-visualization</directory>
      <outputDirectory>doc/graph-visualization</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>../Main/html</directory>
      <outputDirectory>html</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>../X509Security/data</directory>
      <includes>
        <include>truststore.uber</include>
        <include>truststore.jceks</include>
        <include>truststore.p12</include>
        </includes>
      <outputDirectory>data</outputDirectory>
    </fileSet>
  </fileSets>
</assembly>
